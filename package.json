{
  "name": "cloud-storage-sdk",
  "version": "1.0.0-development",
  "description": "A nodejs sdk that supports multiple cloud storage services",
  "keywords": [
    "cloud-storage",
    "oss",
    "aliyun",
    "tencent",
    "qiniu"
  ],
  "homepage": "https://github.com/lzm0x219/cloud-storage-sdk#readme",
  "bugs": {
    "url": "https://github.com/lzm0x219/cloud-storage-sdk/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lzm0x219/cloud-storage-sdk.git"
  },
  "license": "MIT",
  "author": "摸奶伯德",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/esm/index.js"
    },
    "./dist/*": "./dist/*"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc -b",
    "preinstall": "npx only-allow pnpm -y",
    "prepare": "is-ci || husky install"
  },
  "lint-staged": {
    "*.{ts,tsx,js,cjs}": [
      "rome format --write",
      "rome check"
    ],
    "*.{md,html,css,json,yml}": "prettier --write",
    "!(*pnpm-lock).yaml": "prettier --write",
    "package.json": "pnpm sort-package-json"
  },
  "dependencies": {
    "ali-oss": "6.17.1",
    "cos-nodejs-sdk-v5": "2.11.19",
    "qiniu": "7.8.0"
  },
  "devDependencies": {
    "@tsconfig/node16-strictest": "1.0.4",
    "@types/node": "18.11.12",
    "husky": "8.0.2",
    "is-ci": "3.0.1",
    "lint-staged": "13.1.0",
    "prettier": "2.8.1",
    "prettier-plugin-packagejson": "2.3.0",
    "rome": "11.0.0",
    "sort-package-json": "2.1.0",
    "typescript": "4.9.4"
  },
  "peerDependencies": {
    "ali-oss": "6.x",
    "cos-nodejs-sdk-v5": "2.x",
    "qiniu": "7.x"
  },
  "peerDependenciesMeta": {
    "ali-oss": {
      "optional": true
    },
    "cos-nodejs-sdk-v5": {
      "optional": true
    },
    "qiniu": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
